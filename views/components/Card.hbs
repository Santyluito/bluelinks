<article class="card mb-2">

  <h5 class="card-header">{{item.shortURL}}</h5>

  <div class="card-body">
    <p>{{item.origin}}</p>
  </div>

  <div class="card-footer">
    <div class="btn-group" role="group" aria-label="Basic example">
      <a href="/eliminar/{{item._id}}" class="btn btn-danger">Eliminar</a>
      <a href="/editar/{{item._id}}" class="btn btn-warning">Editar</a>
      <button data-short="{{item.shortURL}}" href="#" class="btn btn-dark">Copiar</button>
    </div>
  </div>

</article>